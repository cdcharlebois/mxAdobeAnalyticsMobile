<?xml version="1.0" encoding="utf-8" ?>
<widget id="AdobeAnalytics.widget.AdobeAnalytics" needsEntityContext="true" mobile="true" offlineCapable="true" 
    xmlns="http://www.mendix.com/widget/1.0/">
    <name>AdobeAnalytics</name>
    <description>The description of this widget.</description>
    <icon />
    <properties>
        <property key="dataset" type="object" required="false" isList="true">
            <caption>Data Set (Keys)</caption>
            <category>General</category>
            <description>Data points that you will reference in your event payload</description>
            <properties>
                <property key="dataSourceType" type="enumeration" defaultValue="context" required="true">
                    <caption>Data Source</caption>
                    <category>_General</category>
                    <description>Where does this value come from? (If using anything other than `Context`, configure the appropriate tab.)</description>
                    <enumerationValues>
                        <enumerationValue key="context">Context (Default)</enumerationValue>
                        <enumerationValue key="session">Session (Soon)</enumerationValue>
                        <enumerationValue key="account">Account</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="dataName" type="string" required="false" defaultValue="">
                    <caption>Name</caption>
                    <category>_General</category>
                    <description>{name}</description>
                </property>
                <property key="dataAttribute" type="attribute" required="false" isPath="optional" pathType="reference">
                    <caption>Value</caption>
                    <category>Context (Default)</category>
                    <description>If the Data Source is `Context`, the value at this attribute will replace (`{name}`) in the payload</description>
                    <attributeTypes>
                        <attributeType name="AutoNumber"/>
                        <attributeType name="Boolean"/>
                        <attributeType name="DateTime"/>
                        <attributeType name="Decimal"/>
                        <attributeType name="Enum"/>
                        <attributeType name="Integer"/>
                        <attributeType name="Long"/>
                        <attributeType name="String"/>
                    </attributeTypes>
                </property>
                <property key="dataSessionAttribute" type="string" required="false">
                    <caption>Value (Session)</caption>
                    <category>Session</category>
                    <description>Required if Data Source is "Session"</description>
                </property>
                <property key="dataSessionEntity" type="string" required="false">
                    <caption>Session</caption>
                    <category>Session</category>
                    <description>The session entity. Should be `System.Session`</description>
                </property>
                <property key="dataAccountEntity" type="string" required="false">
                  <caption>Account Entity</caption>
                  <category>Account</category>
                  <description>Should be `Administration.Account`.</description>
                </property>
                <property key="dataAccountAssociation" type="string" required="false">
                  <caption>Association Name</caption>
                  <category>Account</category>
                  <description>The name of the association from the Account Entity to the Target Entity.</description>
                </property>
                <property key="dataAccountTargetEntity" type="string" required="false">
                  <caption>Target Entity</caption>
                  <category>Account</category>
                  <description>The name of the Target Entity. i.e. `ContentManagement.Country`</description>
                </property>
                <property key="dataAccountTargetAttribute" type="string" required="false">
                  <caption>Target Attribute</caption>
                  <category>Account</category>
                  <description>The name of the Targeet Attribute (should be an attribute of the Entity above).</description>
                </property>
            </properties>
        </property>
        <property key="events" type="object" required="false" isList="true">
            <caption>Events</caption>
            <category>General</category>
            <description>Define your events. Reference any values from your dataset between curly braces. i.e. {name}</description>
            <properties>
                <property key="type" type="enumeration" defaultValue="PageLoad">
                    <caption>Type</caption>
                    <category>Event</category>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="PageLoad">Page Load</enumerationValue>
                        <enumerationValue key="GlobalClick">Global Click</enumerationValue>
                        <enumerationValue key="SiblingClick">Sibling Click</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="target" type="string" required="false" defaultValue="">
                    <caption>Target</caption>
                    <category>Event</category>
                    <description>(required if Type is "Click"). The mendix name of the element to trigger the event, i.e. 'actionButton1'</description>
                </property>
                <property key="name" type="string" required="false" defaultValue="">
                    <caption>Event Name</caption>
                    <category>Event</category>
                    <description></description>
                </property>
                <property key="key_Action" type="string" required="false" defaultValue="">
                    <caption>Action</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Country" type="string" required="false" defaultValue="">
                    <caption>Country</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Currency" type="string" required="false" defaultValue="">
                    <caption>Currency</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Value" type="string" required="false" defaultValue="">
                    <caption>Value</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Sector" type="string" required="false" defaultValue="">
                    <caption>Sector</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Appname" type="string" required="false" defaultValue="">
                    <caption>Appname</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <property key="key_Language" type="string" required="false" defaultValue="">
                    <caption>Language</caption>
                    <category>Payload</category>
                    <description></description>
                </property>
                <!-- <property key="payload" type="object" required="false" isList="true"><caption>Event Payload</caption><category>Payload</category><description></description><properties><property key="keyName" type="string" required="true" defaultValue=""><caption>Key Name</caption><category>General</category><description></description></property><property key="keyValue" type="string" required="true" defaultValue=""><caption>Key Value</caption><category>General</category><description></description></property></properties></property> -->
            </properties>
        </property>
    </properties>
</widget>